{
    "description": "GlusterFS plugin for Docker",
    "entrypoint": [
        "/tini",
        "--",
        "/gluster-block-file-plugin"
    ],
    "env": [
        {
            "name": "SERVERS",
            "settable": [
                "value"
            ],
            "value": ""
        },
        {
            "name": "VOLUME_NAME",
            "settable": [
                "value"
            ],
            "value": ""
        },
        {
            "name": "OPTIONS",
            "settable": [
                "value"
            ],
            "value": ""
        },
        {
            "name": "LOGLEVEL",
            "settable": [
                "value"
            ],
            "value": ""
        }
    ],
    "network": {
        "type": "host"
    },
    "mounts": [
        {
            "source": "/dev",
            "destination": "/dev",
            "type": "bind",
            "options": ["rbind"]
        }
    ],
    "propagatedMount": "/mnt",
    "interface": {
        "types": [
            "docker.volumedriver/1.0"
        ],
        "socket": "glusterblockfile.sock"
    },
    "linux": {
        "capabilities": [
            "CAP_SYS_ADMIN"
        ],
        "allowAllDevices": true
    }
}
